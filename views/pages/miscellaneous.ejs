<% include ../partials/header %>
<% include ../partials/navigation %>

<!-- Path widget at top of page-->
<div class="container">
  <div class="path">
  <a href="/">Home</a> / <a href="/loot">Loot</a> / <a id="path-active" href="/loot/miscellaneous">Miscellaneous</a>
  </div>
</div>

<div class="container">
<table class="table table-striped table-dark table-hover">
  <thead>
    <tr>
      <th scope="col">Item</th>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
    </tr>
  </thead>
  <tbody>
    <% for (var i = 0; i < miscellaneous.length; i ++) { %>
      <tr class='clickable-row'>
        <th scope="row"><img class="rarity-background_<%= miscellaneous[i].tier %>" src="<%= miscellaneous[i].image %>" class="img-fluid" alt="<%= miscellaneous[i].name %>"></th>
        <td><%= miscellaneous[i].name %></td>
        <td><%= miscellaneous[i].description %></td> 
      </tr>
    <% } %> 
  </tbody>
</table>
</div>

<% include ../partials/footer %>



